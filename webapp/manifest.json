{
  "_version": "1.58.0",
  "sap.app": {
    "id": "employeestore",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.11.3",
      "toolsId": "67fed0c5-f551-4b5b-a789-f3a869737cb9"
    },
    "dataSources": {
      "users": {
         "uri": "model/UserData.json",
         "type": "JSON"
      },
      "PaymentMethod": {
				"uri": "model/PaymentMethod.json",
				"type": "JSON"
			}
   },
    "crossNavigation": {
      "inbounds": {
        "display-display": {
          "semanticObject": "display",
          "action": "display",
          "title": "{{flpTitle}}",
          "subTitle": "{{flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.119.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "employeestore.i18n.i18n"
        }
      },
      "UsersModel": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "users"
     },
     "PaymentMethod": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "PaymentMethod"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "type": "View",
        "viewType": "XML",
        "controlId": "layout",
        
        "controlAggregation": "midColumnPages",
        "viewPath": "employeestore.view",
        "clearControlAggregation": false,
        "bypassed": {
          "target": [
            "Main"
          ]
        }
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": "",
          "target": [
            "TargetMain"
          ]
        },
        {
          "pattern": "OrderDetail/{orderId}",
          "name": "OrderDetail",
          "target": "OrderDetail"
        },

        {
          "pattern": "ProductDetail/{ProductId}",
          "name": "ProductDetail",
          "target": "ProductDetail"
        },
        {
          "pattern": "PaymentProfileDetail/{mode},{PaymentProfileId}",
          "name": "PaymentProfileDetail",
          "target": "PaymentProfileDetail"
        },
        {
          "pattern": "checkout/{OrderId}",
          "name": "checkout",
          "target": "checkout"
        },
        {
          "pattern": "Cart",
          "name": "Cart",
          "target": [
            "Cart"
          ]
        },
        {
          "pattern": "ordercompleted",
          "name": "ordercompleted",
          "target": "ordercompleted"
        },
        {
          "pattern": "Orders",
          "name": "Orders",
          "target": "Orders"
        },
        {
          "pattern": "PaymentProfiles",
          "name": "PaymentProfiles",
          "target": "PaymentProfiles"
        },
        {
          "pattern": "PaymentCourts",
          "name": "PaymentCourts",
          "target": "PaymentCourts"
        }
      ],
      "targets": {
        "TargetMain": {
          "clearControlAggregation": true,
          "viewType": "XML",
          "controlId": "layout",
          "transition":"slide",
          "controlAggregation": "beginColumnPages",
          "viewId": "Main",
          "viewName": "Main"
        },
        "checkout": {
          
          "level": 1,
          "title": "{i18n>checkoutTitle}",
          "id": "Checkout",
          "viewName": "Checkout",
          "controlId": "app",
          "controlAggregation":"pages",

          "viewType": "XML"
        },
        "ordercompleted": {
          "controlId": "app",
          "controlAggregation":"pages",
          "id": "Ordercompleted",
          "title": "{i18n>orderCompletedTitle}",
          "viewType": "XML",
          "viewName": "OrderCompleted"
        },
        "Orders":{
          "clearControlAggregation": true,
          "viewID": "Orders",
          "viewName": "Orders",
          "transition":"slide",
          "level":1,
          "id": "Orders",
          "viewType": "XML",
          "title": "i18n>OrderViewTitle" 
        },

        "PaymentProfiles":{
          "clearControlAggregation": true,
          "viewID": "PaymentProfiles",
          "viewName": "PaymentProfiles",
          "controlId" : "app",
          "controlAggregation":"pages",
          "transition":"slide",
          "level":1,
          "id": "PaymentProfiles",
          "viewType": "XML",
          "title": "Perfiles de Pago" 
        },

        "PaymentProfileDetail":{
            "viewId": "PaymentProfileDetail",
            "viewName": "PaymentProfileDetail",
            "controlId" : "app",
            "controlAggregation":"pages",
            "transition":"slide",
            "viewType": "XML",
            "title": "Detalle de Perfil de Pago"
        },

        "OrderDetail":{
          "viewId": "OrderDetail",
          "viewName": "OrderDetail",
          "controlId" : "app",
          "controlAggregation":"pages",
          "transition":"slide",
          "level": 1,
          "viewType": "XML",
          "clearControlAggregation": true,
          "title": "i18n>OrderDetailViewTitle"

        },

        "ProductDetail": {
            "viewId": "ProductDetail",
            "viewName": "ProductDetail",
            "controlId" : "app",
            "controlAggregation":"pages",
            "transition":"slide",
            "viewType": "XML",
            "title": "i18n>OrderDetailViewTitle"
        },

        "Cart": {
          
          "viewId": "Cart",
          "clearControlAggregation": true,
          "id": "Cart",
          "viewName": "Cart",
          "viewType": "XML"
          
        },
        "PaymentCourts": {
          
          "level": 1,
          "title": "Cortes de Pago de Productos",
          "id": "PaymentCourts",
          "viewName": "PaymentCourts",
          "controlId": "app",
          "controlAggregation":"pages",
          "viewType": "XML"
        }
      }
    },
    "rootView": {
      "viewName": "employeestore.view.App",
      "type": "XML",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "employeestore"
  }
}
